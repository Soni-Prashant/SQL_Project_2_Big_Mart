/* 1) WRITE a sql query to show all Item_Identifier. */ 
SELECT 
Item_Identifier
FROM Big_Mart

/* 2) WRITE a sql query to show count of total Item_Identifier. */
SELECT 
COUNT(Item_Identifier) AS TotalCount
FROM Big_Mart

/* 3) WRITE a sql query to show maximum Item Weight. */ 
SELECT 
MAX(Item_Weight) AS MaxWeight
FROM Big_Mart

/* 4) WRITE a query to show minimun Item Weight. */
SELECT 
MIN(Item_Weight) AS MinWeight
FROM Big_Mart

/* 5) WRITE a query to show average Item_Weight.*/
SELECT 
ROUND(AVG(Item_weight),2) AS AvgWeight
FROM Big_Mart

/* 6) WRITE a query to show count OF Item_Fat_Content WHERE Item_Fat_Content IS Low Fat.*/
SELECT 
COUNT(Item_Fat_Content)
FROM Big_Mart
WHERE Item_Fat_Content = 'Low Fat' 

/* 7) WRITE a query to show count OF Item_Fat_Content WHERE Item_Fat_Content IS Regular.*/
SELECT 
COUNT(Item_Fat_Content)
FROM Big_Mart
WHERE Item_Fat_Content = 'Regular'

/* 8) WRITE a query TO show maximum Item_MRP.*/ 
SELECT 
MAX(Item_MRP) AS MaxMRP
FROM Big_Mart

/* 9) WRITE a query TO show minimum Item_MRP.*/
SELECT 
MIN(Item_MRP) AS MinMRP
FROM Big_Mart

/* 10) WRITE a query to show Item_Identifier , Item_Fat_Content,Item_Type,Item_MRP and Item_MRP IS greater than 200.*/
SELECT 
Item_Identifier,
Item_Fat_Content,
Item_Type,
Item_MRP
FROM Big_Mart
WHERE Item_MRP &gt; 200

/* 11) WRITE a query to show maximum Item_MRP WHERE Item_Fat_Content IS Low Fat.*/
SELECT 
MAX(Item_MRP) AS MRP
FROM Big_Mart
WHERE Item_Fat_Content = 'Low Fate'

/* 12) WRITE a query to show minimum Item_MRP AND Item_Fat_Content IS Low Fat.*/
SELECT 
MIN(Item_MRP) AS MRP
FROM Big_Mart
WHERE Item_Fat_Content = 'Low Fate'

/* 13) WRITE a query to show ALL DATA WHERE item MRP IS BETWEEN 50 TO 100.*/
SELECT 
*
FROM Big_Mart
WHERE Item_MRP BETWEEN 50 AND 100

/* 14) WRITE a query to show ALL UNIQUE value Item_Fat_Content.*/
SELECT
DISTINCT(Item_Fat_Content)
FROM Big_Mart

/* 15) WRITE a query to show ALL UNIQUE value Item_Type.*/
SELECT
DISTINCT(Item_Type)
FROM Big_Mart

/* 16) WRITE a query to show ALL DATA IN descending ORDER BY Item MRP.*/
SELECT 
*
FROM Big_Mart
ORDER BY Item_MRP DESC 

/* 17) WRITE a query to show ALL DATA IN ascending ORDER BY Item_Outlet_Sales.*/
SELECT 
*
FROM Big_Mart
ORDER BY Item_Outlet_Sales ASC

/* 18) WRITE a query to show ALL DATA IN ascending BY Item_Type.*/
SELECT 
*
FROM Big_Mart
ORDER BY Item_Type

/* 19) WRITE a query to show DATA OF item_type dairy &amp; Meat.*/
SELECT 
*
FROM Big_Mart
WHERE Item_Type IN ('Dairy','Meat')

/* 20) WRITE a query to show ALL UNIQUE value OF Outlet_Size.*/
SELECT 
DISTINCT(Outlet_Size) AS OutletSize
FROM Big_Mart

/* 21) WRITE a query to show ALL UNIQUE value OF Outlet_Location_Type.*/
SELECT 
DISTINCT(Outlet_Location_Type) AS OutletLocationType
FROM Big_Mart

/* 22) WRITE a query to show ALL UNIQUE value OF Outlet_Type.*/
SELECT 
DISTINCT(Outlet_Type) AS OutletType
FROM Big_Mart

/* 23) WRITE a query to show count NO. OF item BY Item_Type AND ordered it IN descending.*/ 
SELECT
Item_Type,
COUNT(Item_type) AS NoofItem 
FROM Big_Mart
GROUP BY Item_Type
ORDER BY COUNT(Item_type) DESC

/* 24) WRITE a query to show count NO. OF item BY Outlet_Size AND ordered it IN ascending.*/
SELECT
Outlet_Size,
COUNT(Outlet_Size) AS OutletCount
FROM Big_Mart
GROUP BY Outlet_Size
ORDER BY Outlet_Size 

/* 25) WRITE a query TO  show  count NO. OF item BY Outlet_Establishment_Year AND ordered it IN ascening.*/ 
SELECT 
Outlet_Establishment_Year,
COUNT(Outlet_Establishment_Year)
FROM Big_Mart
GROUP BY Outlet_Establishment_Year
ORDER BY Outlet_Establishment_Year ASC 

/*26) WRITE a query to show count NO. OF item BY Outlet_Type AND ordered it IN descending.*/
SELECT
Outlet_Type,
COUNT(Outlet_Type)
FROM Big_Mart
GROUP BY Outlet_Type
ORDER BY Outlet_Type DESC 

/* 27) WRITE a query to show maximum MRP BY Item_Type.*/
SELECT
MAX(Item_MRP) AS MaxMRP,
Item_Type
FROM Big_Mart
GROUP BY Item_Type

/* 28) WRITE a query to show average MRP BY Outlet_Size AND ordered IN descending.*/ 
SELECT 
ROUND(AVG(Item_MRP),2) AS MRP,
Outlet_Size 
FROM Big_Mart
GROUP BY Outlet_Size
ORDER BY AVG(Item_MRP) DESC

/* 29) WRITE a query to show maximum Item_Outlet_Sales BY Item_Type.*/
SELECT 
MAX(Item_Outlet_Sales) AS Sales,
Item_Type
FROM Big_Mart
GROUP BY Item_Type

/* 30) WRITE a query to show total Item_Outlet_Sales BY Item_Type AND ORDER it descending.*/
SELECT 
COUNT(Item_Outlet_Sales) TotalSales
FROM Big_Mart
GROUP BY Item_Type
ORDER BY Item_Type DESC

/* 31) WRITE a query to show total Item_Outlet_Sales BY Item_Type but ONLY WHERE Outlet_Location_Type IS Tier 1.*/ 
SELECT 
COUNT(Item_Outlet_Sales) AS TotalSales,
Outlet_Location_Type
FROM Big_Mart
WHERE Outlet_Location_Type = 'Tier 1'
GROUP BY Outlet_Location_Type

/* 32) WRITE a query to show total Item_Outlet_Sales BY Item_Type WHERE Item_Fat_Content IS ONLY Low Fat &amp; LF.*/
SELECT 
COUNT(Item_Outlet_Sales) AS TotalSales,
Item_Type
FROM Big_Mart
WHERE Item_Fat_Content IN ('Low Fat', 'LF')
GROUP BY Item_Type
ORDER BY COUNT(Item_Outlet_Sales)
</sql><current_tab id="0"/></tab_sql></sqlb_project>
